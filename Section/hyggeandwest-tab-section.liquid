

{% liquid
  assign laptop_padding_top = section.settings.laptop_top
  assign laptop_padding_bottom = section.settings.laptop_bottom
  assign mobail_padding_top = section.settings.mobail_top
  assign mobail_padding_bottom = section.settings.mobail_bottom
  assign bgcolor = section.settings.bgcolor
%}

<style>
  .id_{{ section.id}}{
    background-color: {{bgcolor}};
    background-image:url({{section.settings.bgimg | image_url}});
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
  }

  .id_{{ section.id}} .container{
      padding-top:{{laptop_padding_top}}px;
      padding-bottom:{{laptop_padding_bottom}}px;
      box-sizing: border-box;
      margin:0px auto;
  }
  {% if section.settings.container == "container" %}
      .id_{{ section.id}} .container{
      max-width:1200px;    
    }
  {% elsif  section.settings.container == "container-full" %}
    .id_{{ section.id}} .container{
      max-width:100%;
      padding-left:2%;
     padding-right:2%;
    }
  {% endif %}



  @media(max-width:767px){
      .id_{{ section.id}}{
            padding-left:20px;
       padding-right:20px;
      }
    .id_{{ section.id}} .container{
      padding-top:{{mobail_padding_top}}px;
      padding-bottom:{{mobail_padding_bottom}}px;
    }
      .id_{{ section.id}} .container{
      max-width:100%;
      padding-left:0;
     padding-right:0;
    }


  }
</style>

<section class="id_{{ section.id }}">
  <div class="container">
        {% for block in section.blocks %}
          {% case block.type %}
                {% when 'title' %}
              <style>
                .{{block.id}}.title{
                  font-size:{{block.settings.font_size}}px;
                  font-weight:{{block.settings.font_weight}};
                  line-height:{{block.settings.line_height}}px;
                  color:{{block.settings.textcolor}};
                  margin-bottom:{{block.settings.margin_bottom}}px !important;
                  text-align: {{block.settings.text_align}};
                  width:{{block.settings.width}}%;
                  margin:{{block.settings.align_content}}
                }
                 @media(max-width:767px){
                 .{{block.id}}.title{
                  font-size:{{block.settings.mobail_font_size}}px;
                  line-height:{{block.settings.mobail_line_height}}px;
                  margin-bottom:{{block.settings.mobail_margin_bottom}}px !important;
                  text-align: {{block.settings.mobail_text_align}};
                  width:{{block.settings.mobail_width}}%;
                  margin:{{block.settings.mobail_align_content}}
                }
                 }
              </style>

              <h3 class="title {{ block.id }}">{{ block.settings.title }}</h3>
              {% when 'richtext' %}
              <style>
                 .{{block.id}}{
                   font-size:{{block.settings.font_size}}px;
                   font-weight:{{block.settings.font_weight}};
                   line-height:{{block.settings.line_height}}px;
                   color:{{block.settings.textcolor}};
                   margin-bottom:{{block.settings.margin_bottom}}px !important;
                  text-align: {{block.settings.text_align}};
                  width:{{block.settings.width}}%;
                  margin:{{block.settings.align_content}}
                 }
                    .{{block.id}} ul{
                             padding-inline-start: 0;
                       margin-block: 0;
                       }
                       .{{block.id}} ul li{
                          list-style: none;
                         margin-bottom:10px;
                          display: flex;
                       }
                 .{{block.id}} ul li::before{

                   content: "\2713";
                   font-size:{{block.settings.font_size}}px;
                   font-weight:{{block.settings.font_weight}};
                   margin-right: 10px;


                   }
                  @media(max-width:767px){
                  .{{block.id}}{
                   font-size:{{block.settings.mobail_font_size}}px;
                   line-height:{{block.settings.mobail_line_height}}px;
                   margin-bottom:{{block.settings.mobail_margin_bottom}}px !important;
                  text-align: {{block.settings.mobail_text_align}};
                  width:{{block.settings.mobail_width}}%;
                  margin:{{block.settings.mobail_align_content}}
                 }
                .{{block.id}} ul li::before{
                      font-size:{{block.settings.mobail_font_size}}px;
                     }
                  }
              </style>
              <div class="{{ block.id }}">{{ block.settings.text }}</div>
                       {% when 'button' %}
              <style>
                .{{block.id}} .btn{
                  font-size:{{block.settings.font_size}}px;
                  font-weight:{{block.settings.font_weight}};
                  line-height:{{block.settings.font_size}}px;
                  color:{{block.settings.text_color}};
                  border-color:{{block.settings.border_color}};
                  border-width:{{block.settings.border_width}}px;
                  border-style:{{block.settings.border_style}};
                  border-radius:{{block.settings.border_radius}}px;
                  background-color: {{block.settings.btn_color}};
                  margin-bottom:{{block.settings.margin_bottom}}px;
                  padding:{{block.settings.padding}};
                  transition-duration: .5s;
                  text-align: center;
                 text-decoration: none;
                 display: inline-block;
                }
                 .{{block.id}} .btn:hover{
                    background-color: {{block.settings.btn_color_hover}};
                    color:{{block.settings.text_color_hover}};
                    border-color:{{block.settings.border_color_hover}};
                 }
                 .{{block.id}}{
                    text-align: {{block.settings.text_align}};
                 }
                 @media(max-width:767px){
                 .{{block.id}} .btn{
                  font-size:{{block.settings.mobail_font_size}}px;
                  line-height:{{block.settings.mobail_font_size}}px;
                  margin-bottom:{{block.settings.mobail_margin_bottom}}px;
                  padding:{{block.settings.mobail_padding}};
                }
                .{{block.id}}{
                    text-align: {{block.settings.mobail_text_align}};
                 }
                 }
              </style>

              {% if block.settings.btnlink != blank %}
                <div class="{{ block.id }}">
                <a class="btn" href="{{ block.settings.btnlink }}">{{ block.settings.btntext }}</a>
                </div>
              {% endif %}
            
    
          {% when 'image' %}
            <style>
              .{{ block.id }}{
                 aspect-ratio: {{block.settings.aspect_ratio}};
                 object-fit: {{block.settings.object_fit}};
                 border-radius:{{block.settings.border_radius}}{{block.settings.border_px}};
                width:{{block.settings.width}};
                margin:0px auto {{block.settings.margin_bottom}}px;
              }
              @media(max-width:767px){
                .{{ block.id }}{
                 width:{{block.settings.mwidth}};
                margin:0px auto {{block.settings.mmargin_bottom}}px;
              }  
              }
            </style>
            
              {% if block.settings.img != blank %}
                <img class="{{ block.id }}"
                  src="{{ block.settings.img | image_url }}"
                  alt="{{ block.settings.img.alt }}"
                  width="100"
                  height="100"
                >
                {% endif %}


             {% when 'teb_section' %}
            
            <style>


               .{{ block.id }}.tab_main_div{
                  max-width:{{block.settings.box_width}};
                 margin: 0px auto;
                 text-align: center;
                 width: 100%;
                 box-sizing: border-box;
                 }
              .{{ block.id }} .tab_header{ display: flex;}
              .{{ block.id }} .tab_header .list_div{ width: 100%; box-sizing: border-box;}
              .{{ block.id }} .tab_header h5 {
                 font-size: 16px;
                 line-height: 100%;
                 font-weight: 600;
                 color: {{block.settings.text_color}};
                 width: 100%;
                 background-color: {{block.settings.bg_color}};
                 padding: 12px 30px;
                 box-sizing: border-box;
                 height: 100%;
                 display: flex;
                 justify-content: center;
                 align-items: center;
                margin:0px;
                }
               .{{ block.id }} .tab_header h5:hover, .{{ block.id }} .tab_header .active{ background-color:{{block.settings.active_color}}; }
               .{{ block.id }} .tab_body{ padding: 50px 0px 0px 0px; }

               .{{ block.id }} .tab_body .tab_box {
                   display: flex;
                   flex-wrap: wrap;
                   margin: -10px;
                   align-items: center;
                   justify-content: center;
                 }
               .{{ block.id }} .tab_body .tab_box .col{
                 max-width: 25%;
                 width: 100%;
                 box-sizing: border-box;
                 padding: 10px;
                 margin: -10px 0px;
                  }
               .{{ block.id }} .tab_body .tab_box .col_img{
                     max-width: 50%;
                     width: 100%;
                     box-sizing: border-box;
                     padding: 10px;
                 }
              .{{ block.id }} .tab_body .tab_box .col_img img {
                   display: block;
                   max-width: 100%;
                   aspect-ratio: 1/1;
                   width: 100%;
                   border-radius: 100%;
                  object-fit: cover;
                  }

               .{{ block.id }} .tab_body .tab_box .col .img_box img{
                     max-width: 100%;
                    display: block;
                    aspect-ratio: 1/1;
                    width: 100%;
                    border-radius: 100%;
                   object-fit: cover;
                  }
               .{{ block.id }} .tab_body .text{
                   font-size: 16px;
                   line-height: 24px;
                   font-weight: 400;
                   margin: 0px 0px 20px;
                 color:{{block.settings.des_color}};
                   }            

                .{{ block.id }} .tab_body  .img_box{ padding:10px 0px; }
                .{{ block.id }} .tab_body  .img_box img{ margin: 0px auto 10px; }
                .{{ block.id }} .tab_body  .img_box p{
                    font-size: 14px;
                    line-height: 20px;
                    font-weight: 400;
                    margin: 0px;  
                   color:{{block.settings.des_color}};
                    }

            @media(max-width:767px){
           .{{ block.id }}.tab_main_div { max-width: 100%;}
           .{{ block.id }} .tab_header{ flex-wrap: wrap; margin: -3px;}
           .{{ block.id }} .tab_header .list_div{
             max-width: 50%;
             box-sizing: border-box;
             padding: 3px;
              }
            .{{ block.id }} .tab_header h5 {
               font-size: 14px;
              line-height: 120%;
              padding: 15px 30px;
               }
            .{{ block.id }} .tab_body {
               padding: 25px 0px 0px 0px;
                }

            .{{ block.id }} .tab_body .tab_box .col {
               max-width: 100%;
               padding: 10px;
               margin: -10px;
               display: flex;
            }

             .{{ block.id }} .tab_body  .img_box {
                  padding: 10px;
                  max-width: 50%;
                  box-sizing: border-box;
                }
              .{{ block.id }} .tab_body .tab_box .col_img { max-width: 100%;}
               .{{ block.id }} .tab_body .tab_box {
                 max-width: 90%;
                 width: 100%;
                 margin: -10px auto;
                }

                }

            
            </style>

             <div class="tab_main_div {{ block.id }}">
           <div class="tab_header">
             {% for block in section.blocks %}
               {% case block.type %}
                {% when 'teb_list' %}
                <div class="list_div">
                  <h5 class="tabtop" onclick="openPage('{{ block.id }}', this )"  
                    {%- if block.settings.active_teb -%}
                         id="defaultOpen"
                      {%- endif -%}
                    >{{ block.settings.name }}</h5>
               </div>
                  {% endcase %}
            {% endfor %}
               
           </div>
                {% for block in section.blocks %}
               {% case block.type %}
                {% when 'teb_list' %}

           <div class="tab_body tabcontent" id="{{ block.id }}">
                <p class="text">{{ block.settings.des }}</p>

                <div class="tab_box">
                    <div class="col">
                        <div class="img_box">
                           {% if block.settings.img1 != blank %}
                           <img 
                           src="{{ block.settings.img1 | image_url }}"
                           alt="{{ block.settings.img1.alt }}"
                           width="100%"
                           height="100%"
                          >
                       {% endif %}
                            <p>{{ block.settings.text1 }}</p>
                        </div>

                        <div class="img_box">
                            {% if block.settings.img3 != blank %}
                          <img 
                           src="{{ block.settings.img3 | image_url }}"
                           alt="{{ block.settings.img3.alt }}"
                           width="100%"
                           height="100%"
                       >
                    {% endif %}
                            <p>{{ block.settings.text3 }}</p>
                        </div>

                    </div>
                    <div class="col_img">
                         {% if block.settings.img != blank %}
                      <img
                           src="{{ block.settings.img | image_url }}"
                           alt="{{ block.settings.img.alt }}"
                           width="100%"
                           height="100%"
                       >
                    {% endif %}
                    </div>

                    <div class="col">
                        <div class="img_box">
                           {% if block.settings.img2 != blank %}
                           <img 
                           src="{{ block.settings.img2 | image_url }}"
                           alt="{{ block.settings.img2.alt }}"
                           width="100%"
                           height="100%"
                          >
                       {% endif %}
                            <p>{{ block.settings.text2 }}</p>
                        </div>

                        <div class="img_box">
                            {% if block.settings.img4 != blank %}
                          <img 
                           src="{{ block.settings.img4 | image_url }}"
                           alt="{{ block.settings.img4.alt }}"
                           width="100%"
                           height="100%"
                       >
                    {% endif %}
                            <p>{{ block.settings.text4 }}</p>
                        </div>

                    </div>
                </div>
               
           </div>
                {% endcase %}
            {% endfor %}

               
  </div>
          

          {% endcase %}
      {% endfor %} 
    
  </div>
</section>

<script>
    function openPage(pageName,elmnt,color) {
      var i, tabcontent, tablinks;
      tabcontent = document.getElementsByClassName("tabcontent");
      for (i = 0; i < tabcontent.length; i++) {
        tabcontent[i].style.display = "none";
      }
      tablinks = document.getElementsByClassName("tabtop");
      for (i = 0; i < tablinks.length; i++) {
        tablinks[i].classList.remove("active");
      }
      document.getElementById(pageName).style.display = "block";
      elmnt.classList.add("active");
    }
    
    // Get the element with id="defaultOpen" and click on it
    document.getElementById("defaultOpen").click();
    </script>










{% schema %}
